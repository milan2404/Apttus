<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Apttus__Document_Password_Validation</fullName>
    <active>true</active>
    <description>Document password cannot be blank</description>
    <errorConditionFormula>IF(AND( Name == &quot;APTS_Password&quot;, Apttus__Value__c == &quot;&quot;), true, false)</errorConditionFormula>
    <errorDisplayField>Apttus__Value__c</errorDisplayField>
    <errorMessage>Password cannot be blank</errorMessage>
</ValidationRule>
